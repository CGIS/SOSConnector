<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Configure className="EPA">
    <info>
        <url>http://opendata.epa.gov.tw/ws/Data/RainTenMin/?%24orderby=PublishTime%20desc&amp;%24skip=0&amp;%24top=1000&amp;format=xml</url>
        <sos>http://localhost:8080/epa_rainfall/service</sos>
        <xmlDataRoot>Data</xmlDataRoot>
        <period>
            <!--<sec>4</sec>-->
            <min>10</min>
            <!--<hour>6</hour> -->
            <!--<day>7</day>-->
            <!--<week>8</week>-->
            <!--<month>9</month>-->
        </period>
    </info>

    <insertSensor>
        <offering>SiteName</offering>
        <procedure>SiteId</procedure>
        <prefix>EPA</prefix>
        <propertyPrefix>urn:ogc:def:phenomenon:OGC:2.0:EPA:</propertyPrefix>
        <property>Rainfall10min</property>
        <property>Rainfall1hr</property>
        <!--<longitude>lon</longitude>-->
        <!--<latitude>lat</latitude>-->
    </insertSensor>

    <insertObservation>
        <longitude>TWD67Lon</longitude>
        <latitude>TWD67Lat</latitude>
        <timestamp>PublishTime</timestamp>
        <observation name="Rainfall10min" type="gml:MeasureType" unit="mm">Rainfall10min</observation>
        <observation name="Rainfall1hr" type="gml:MeasureType" unit="mm">Rainfall1hr</observation>
    </insertObservation>
</Configure>