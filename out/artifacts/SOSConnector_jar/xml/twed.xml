    <sos>http://localhost:8080/twed/service</sos>
    <xmlDataRoot>twed:RealtimeWaterLevel</xmlDataRoot>
    <period>
        <!--<sec>4</sec>-->
        <min>10</min>
        <!--<hour>6</hour> -->
        <!--<day>7</day>-->
        <!--<week>8</week>-->
        <!--<month>9</month>-->
