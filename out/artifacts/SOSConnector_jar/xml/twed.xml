</info>

<insertSensor>
    <offering>twed:StationIdentifier</offering>
    <procedure>twed:StationIdentifier</procedure>
    <prefix>TWED</prefix>
    <propertyPrefix>urn:ogc:def:phenomenon:OGC:2.0:twed:</propertyPrefix>
    <property>waterLevel</property>
</insertSensor>

<insertObservation>
    <timestamp>twed:RecordTime</timestamp>
    <observation name="waterLevel" type="gml:MeasureType" unit="mm">twed:WaterLevel</observation>
</insertObservation>
</Configure>