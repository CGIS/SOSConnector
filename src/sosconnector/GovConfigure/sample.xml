<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Configure className="Sample">
    <info>
        <url>http://data.wra.gov.tw/Service/OpenData.aspx?id=2D09DB8B-6A1B-485E-88B5-923A462F475C&format=xml</url>
        <sos>http://localhost:8080/twed_waterLevel/service</sos>
        <xmlDataRoot>data</xmlDataRoot>
        <period>
            <!--<sec>4</sec>-->
            <!--<min>5</min>-->
            <!--<hour>6</hour> -->
            <day>7</day>
            <!--<week>8</week>-->
            <!--<month>9</month>-->
        </period>
    </info>

    <insertSite>
        <offering>Site</offering>
        <procedu>procedure</procedu>
        <prefix>AQX</prefix>
        <propertyPrefix>urn:ogc:def:phenomenon:OGC:2.0:AQX:</propertyPrefix>
        <property>CO</property>
        <property>PM2.5</property>
        <!--<longitude>lon</longitude>-->
        <!--<latitude>lat</latitude>-->
    </insertSite>

    <insertObservation>
        <longitude>lon</longitude>
        <latitude>lat</latitude>
        <observation name="CO" type="measure" unit="mm">CO</observation>
        <observation name="PM2.5" type="measure" unit="mm">PM2.5</observation>
    </insertObservation>
</Configure>